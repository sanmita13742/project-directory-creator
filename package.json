{
  "name": "project-directory-creator",
  "displayName": "Project Directory Creator",
  "description": "Creates project folder structures based on templates or custom input.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createProjectStructure",
        "title": "Create Project Directory"
      }
    ]
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "vscode": "^1.1.37"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.5.5",
    "mocha": "^10.7.3",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "compile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanmita13742/project-directory-creator"
  }
}
